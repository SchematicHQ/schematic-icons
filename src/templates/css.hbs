@font-face {
  font-family: "{{name}}";
  src: url(data:font/truetype;charset=utf-8;base64,{{assets.ttf}}) format("truetype");
}

.{{prefix}} {
  font-family: {{name}} !important;
  font-style: normal;
  font-weight: normal !important;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
.{{../prefix}}-{{@key}}:before {
  content: "\\{{this}}";
}
{{/each}} 